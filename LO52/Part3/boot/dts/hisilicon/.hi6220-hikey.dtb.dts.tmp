  vmmc-supply = <&wlan_en_reg>;
  pinctrl-names = "default", "idle";
  pinctrl-0 = <&sdio_pmx_func &sdio_clk_cfg_func &sdio_cfg_func>;
  pinctrl-1 = <&sdio_pmx_idle &sdio_clk_cfg_idle &sdio_cfg_idle>;
 };

 wlcore {
  compatible = "wlcore";
# 157 "arch/arm64/boot/dts/hisilicon/hi6220-hikey.dts"
  gpio = <499>;

  platform-quirks = <1>;

  board-ref-clock = <4>;
 };

 kim {
  compatible = "kim";
  pinctrl-names = "default";
  pinctrl-0 = <>;







  nshutdown_gpio = <503>;
  dev_name = "/dev/ttyAMA1";
  flow_cntrl = <1>;
  baud_rate = <3000000>;
 };

 btwilink {
  compatible = "btwilink";
 };

 leds {
  compatible = "gpio-leds";
  user_led1 {
   label = "user_led1";
   gpios = <&gpio4 0 0>;
   linux,default-trigger = "heartbeat";
  };

  user_led2 {
   label = "user_led2";
   gpios = <&gpio4 1 0>;
   linux,default-trigger = "mmc0";
  };

  user_led3 {
   label = "user_led3";
   gpios = <&gpio4 2 0>;
   linux,default-trigger = "mmc1";
  };

  user_led4 {
   label = "user_led4";
   gpios = <&gpio4 3 0>;
   linux,default-trigger = "cpu0";
  };

  wlan_active_led {
   label = "wifi_active";
   gpios = <&gpio3 5 0>;
   linux,default-trigger = "phy0tx";
   default-state = "off";
  };

  bt_active_led {
   label = "bt_active";
   gpios = <&gpio4 7 0>;
   linux,default-trigger = "hci0rx";
   default-state = "off";
  };

 };


 wlan_en_reg: fixedregulator@1 {
  compatible = "regulator-fixed";
  regulator-name = "wlan-en-regulator";
  regulator-min-microvolt = <1800000>;
  regulator-max-microvolt = <1800000>;


  gpio = <&gpio0 5 0>;


  startup-delay-us = <70000>;
  enable-active-high;
 };

        hisi-ion@0 {
                compatible = "hisilicon,ion";

                heap_sys_user@0 {
                        heap-name = "sys_user";
                        heap-range = <0x0 0x0>;
                        heap-type = "ion_system";
                };

                heap_sys_contig@0 {
                        heap-name = "sys_contig";
                        heap-range = <0x0 0x0>;
                        heap-type = "ion_system_contig";
                };

                heap_cma@0 {
                        heap-name = "cma";
                        heap-range = <0x0 0x0>;
                        heap-type = "ion_cma";
                };
        };
};
