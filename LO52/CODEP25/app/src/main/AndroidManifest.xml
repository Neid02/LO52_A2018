<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.silentpangolin.codep25">

    <uses-feature
        android:name="android.hardware.sensor.accelerometer"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PlayerActivity"
            android:label="@string/listcrr"
            android:noHistory="true" />
        <activity
            android:name=".TeamActivity"
            android:label="@string/listequ" />
        <activity
            android:name=".CreditsActivity"
            android:label="@string/credits" />
        <activity
            android:name=".RankingPlayerActivity"
            android:label="@string/rankingPlayer" />
        <activity
            android:name=".RankingTeamActivity"
            android:label="@string/rankingTeam" />
    </application>

</manifest>